<!DOCTYPE html>
<html>
  <head>
    <title>Blog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

    <body id = "library">
        <div id = "container">

        <div id = "header">
            <h1>Articles</h1>
            <h3>Powered by Ruby on Rails</h3>
        </div>

        <div id = "content">
            <%= yield %>
        </div>

        <div id = "sidebar"></div>

        </div>
    </body>
</html>
